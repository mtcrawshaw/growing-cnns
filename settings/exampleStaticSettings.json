{
    "growing": false,

    "initialChannels": 64,
    "numSections": 4,
    "initialNumNodes": 3,

    "growthSteps": 3,
    "growthMode": "expandEdge",
    "numConvToAdd": 1,
    "itemsToExpand": "youngest",
    "join": {
	    "type": "uniform",
    	    "preserve": 0.95
    },
    "stopping": {
	    "type": "fixedEpochs",
	    "numEpochs": 3
    },

    "batchSize": 64,
    "initialLearningRate": 0.01,
    "momentum": 0.9,
    "weightDecay": 1e-4,
    "batchNorm": true,

    "lrDecayStep": 5,
    "lrDecayRatio": 0.1
}
